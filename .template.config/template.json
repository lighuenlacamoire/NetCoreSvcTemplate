{
  "$schema": "http://json.schemastore.org/template",
  "author": "Banco Galicia",
  "name": "API Banco Galicia Template", // Valor que figura en la lista de templates - dotnet new --list 
  "description": "Template para los microservicios de Galicia Seguros",
  "identity": "GaliciaSegurosSvcTemplate.NetCore",
  "shortName": "GaliciaSegurosSvcTemplate", // Valor utilizado en el cli - dotnet new {shortName}
  "classifications": ["Web"],
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "GaliciaSegurosSvcTemplate",
  "symbols": {
    "serviceName": {
      "type": "parameter",
      "description": "Servicio: Nombre",
      "defaultValue": "{SERVICE_NAME}",
      "replaces": "{SERVICE_NAME}"
    },
    "serviceDescription": {
      "type": "parameter",
      "description": "Servicio: Descripcion",
      "defaultValue": "{SERVICE_DESCRIPTION}",
      "replaces": "{SERVICE_DESCRIPTION}"
    },
    "serviceVersion": {
      "type": "parameter",
      "description": "Servicio: Nº de Version",
      "defaultValue": "{SERVICE_VERSION}",
      "replaces": "{SERVICE_VERSION}"
    },
    "Framework": {
      "type": "parameter",
      "description": "Version de Netcore",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0"
        }
      ],
      "defaultValue": "net6.0"
    }
  }
}