services:
  galiciaseguros.iaas.service.template.ddd.api:
    build:
      context: D:\GIT\OWNER\NetCoreSvcTemplate
      dockerfile: src/GaliciaSeguros.IaaS.Service.Template.DDD.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: GaliciaSeguros.IaaS.Service.Template.DDD.API
      target: base
    container_name: GaliciaSeguros.IaaS.Service.Template.DDD.API
    depends_on:
      mongo:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: galiciasegurosiaasservicetemplatedddapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/GaliciaSeguros.IaaS.Service.Template.DDD.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5156
      target: 80
    - published: 7156
      target: 443
    tty: true
    volumes:
    - D:\GIT\OWNER\NetCoreSvcTemplate\src\GaliciaSeguros.IaaS.Service.Template.DDD.API:/app:rw
    - C:\Users\Administrador\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Administrador\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Administrador\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Administrador\.nuget\packages:/root/.nuget/packages:ro
    - D:\GIT\OWNER\NetCoreSvcTemplate:/src:rw
  mongo:
    environment:
      MONGO_INITDB_ROOT_PASSWORD: example
      MONGO_INITDB_ROOT_USERNAME: root
    image: mongo
    ports:
    - published: 27017
      target: 27017
    restart: always
  mongo-express:
    depends_on:
      mongo:
        condition: service_started
    environment:
      ME_CONFIG_MONGODB_ADMINPASSWORD: example
      ME_CONFIG_MONGODB_ADMINUSERNAME: root
    image: mongo-express
    ports:
    - published: 8081
      target: 8081
    restart: always
version: '3.4'